<BA>
  <audio>
    <buffers>4</buffers>
  </audio>

  <terminal>
   <escapeSequence>false</escapeSequence>
  </terminal>

  <code>
    <key char="t"><![CDATA[
      var filter = Hires(1, 2);
      BA_UT_ok(filter, "Hires() exists.");
      BA_UT_is(filter.name, "hires", "Hires().name == 'hires'");
      BA_UT_is(filter.cutOff, 1, "Hires().cutOff == 1");
      BA_UT_is(filter.resonance, 2, "Hires().resonance == 2");

      function testAttr(obj, name) {
        var value = Math.random();
        obj[name] = value;
        BA_UT_is(obj[name], value, obj.name + "." + name + " == " + value);

        try {
          obj[name] = "foobar";
          BA_UT_ok(false, obj.name + "." + name + " = foo should throw");
        } catch(e) {
          BA_UT_ok(true, obj.name + "." + name + " = foo should throw");
        }
      }

      testAttr(filter, 'cutOff');
      testAttr(filter, 'resonance');

      try {
        filter.update();
        BA_UT_ok(false, "Hires.update() with no arguments should throw");
      } catch(e) {
        BA_UT_ok(true, "Hires.update() with no arguments should throw");
      }

      try {
        filter.update("foo", "bar");
        BA_UT_ok(false, "Hires.update() with wrong arguments should throw");
      } catch(e) {
        BA_UT_ok(true, "Hires.update() with wrgon arguments should throw");
      }

      filter.update(11, 22);
      BA_UT_is(filter.cutOff, 11, "Hires().cutOff == 11");

      filter.update(11, 22);
      BA_UT_is(filter.cutOff, 11, "Hires().cutOff == 11");
      BA_UT_is(filter.resonance, 22, "Hires().resonance == 22");

      quit();
    ]]>
    </key>
  </code>
</BA>
