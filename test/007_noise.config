<BA>
  <audio>
    <buffers>4</buffers>
  </audio>

  <terminal>
   <escapeSequence>false</escapeSequence>
  </terminal>

  <code>
    <key char="t"><![CDATA[
      currentBuffer.play("noise");

      var intervals = 10;
      var iId = setInterval(function() {
        BA_UT_is(currentBuffer.dataType, "noise", "We are playing noise - " + intervals);
        if (!--intervals) {
          clearInterval(iId);
          quit();
          next();
        }
      }, 200);
    ]]>
    </key>
  </code>
</BA>


